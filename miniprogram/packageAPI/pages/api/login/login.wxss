.page-section {
  margin-top: 20px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  width: 100%;
  padding: 0 25px;
  box-sizing: border-box;
}

.page-body-title {
  font-size: 30px;
  line-height: 40px;
}

.page-body-text {
  color: #bbb;
  font-size: 14px;
  line-height: 20px;
  margin: 0 0 0 0;
  text-align: center;
}

.page-body-button {
  width: 100%;
}